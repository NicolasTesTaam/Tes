name: CI/CD
   
on:
  push:
    branches:
      - main
job:
ci:
  runs-on: ubuntu-latest
  steps:
    -name: Checkout code
    user: actions/checkout@v4
    -name: Run Dumm Test (Placeholder)
    run: echo "Code check Passes"
deploy:
  needs: ci
  runs-on: ubuntu-latest
  permissions:
    Pages: write
    id-token: write
  steps:
  -name: Checkout code
  uses: actions/checkout@v4
  -name: Setup Pages
  uses: actions/configure-pages@v4
  -name: Upload to Artifacts
  uses: actions/upload-pages-artifact@v3
  with:
    path:
  -name: Deploy to GitHub Pages
  id: deploument
  user : actions/deploy-pages@v4
  -name: Deployment Successful
  run: echo "Deployed Successfully"


 